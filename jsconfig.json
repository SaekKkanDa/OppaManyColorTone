{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "baseUrl": ".",
    "paths": {
      "@Components/*": ["./src/components/*"],
      "@Constant/*": ["./src/constant/*"],
      "@Hooks/*": ["./src/hooks/*"],
      "@Pages/*": ["./src/pages/*"],
      "@Utils/*": ["./src/utils/*"],
      "@Styles/*": ["./src/styles/*"]
    },
    "checkJs": true,
    "jsx": "react"
  },
  "include": ["src/**/*.js", "src/**/*.jsx"],
  "exclude": ["node_modules"]
}
